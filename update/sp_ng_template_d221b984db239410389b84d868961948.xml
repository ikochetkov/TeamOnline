        <template><![CDATA[<div class="comment-input-container">
  <div class="comment-reply-to" ng-if="ctrl.parent">
    <span class="reply-to-parent"><i class="fa fa-reply"></i> {{ctrl.parentText}}</span>
    <button class="btn btn-default" type="button" ng-click="ctrl.cancelParent()">
      <i class="fa fa-times"></i>
    </button>
  </div>
  <form>
    <div class="input-group">
      <textarea type="text" class="form-control" id="comment-input" placeholder="Type comment here..." ng-model="ctrl.comment" style="resize: none; border-bottom: none" maxlength="140" >
      <span class="input-group-btn">
        <button class="btn btn-default" type="button" ng-click="ctrl.send()" style="margin-left: 0;">
          <i class="fa fa-paper-plane-o"></i>
        </button>
      </span>
    </div>
  </form>
</div>]]></template>
