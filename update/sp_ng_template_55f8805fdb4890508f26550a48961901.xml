        <template><![CDATA[<div class="megamenu-bg">
  <div class="container">
    <div class="row">
      <button ng-if="data.canEdit" class="btn btn-add-subcategory btn-lock i-b" ng-click="allowEdit()">
        <fa ng-if="!data.linkEditAllowed" name="lock"></fa>
        <fa ng-if="data.linkEditAllowed" name="unlock"></fa>
      </button>
      <div ng-repeat="sitem in item.items | orderBy:'order'">
        <h3 class="font-weight-bold text-uppercase" style="text-align: left">
          <a class="megamenu__category color-white" ng-href="{{::sitem.url}}" style="display:inline; "
            target="_self">{{::sitem.label}}</a>
          <a class="color-white" ng-if="data.canEdit&&data.linkEditAllowed" href=""
            ng-click="openLinkEditor('', 'Create', sitem)" style="font-size: 10pt; margin-left:10px">
            <fa name="plus-circle"></fa>
            Add new Link
          </a>
          <a class="color-white" ng-if="data.canEdit&&data.linkEditAllowed" href=""
            ng-click="openLinkEditor(sitem, 'Edit')" style="font-size: 10pt; margin-left:10px">
            <fa name="edit"></fa>
            Edit folder
          </a>
          <a class="color-white" ng-if="data.canEdit&&data.linkEditAllowed" href=""
            ng-click="deleteRecord(sitem, 'Folder')" style="font-size: 10pt; margin-left:10px">
            <fa name="trash"></fa>
            Remove folder
          </a>
        </h3>
        <div ng-if="item.items.length > 0" class="subheading-list three-column-layout column-fill-auto">
          <div class="subheading-list-item" ng-repeat="subitem in sitem.items | orderBy: 'order'">
            <a class="megamenu-subcategory semi-transparent" ng-href="{{subitem.url}}" ng-click="openUrl(subitem.url)">
              <fa ng-if="::subitem.glyph" name="{{::subitem.glyph}}"></fa>
              {{::subitem.label}}
            </a>
            <a href="" style="float: right; margin-right:10px;" class="megamenu-subcategory semi-transparent"
              ng-click="deleteRecord(subitem, 'Link')">
              <fa ng-if="data.canEdit&&data.linkEditAllowed" name="trash"></fa>
            </a>
            <a href="" style="float: right; margin-right:10px;" class="megamenu-subcategory semi-transparent"
              ng-click="openLinkEditor(subitem, 'Edit')">
              <fa ng-if="data.canEdit&&data.linkEditAllowed" name="edit"></fa>
            </a>

          </div>
        </div>
      </div>
      <h3 class="font-weight-bold text-uppercase" style="text-align: left">
        <button ng-if="data.canEdit&&data.linkEditAllowed" class="btn btn-add-subcategory i-b"
          ng-click="openLinkEditor('','Create', item)">
          <fa name="plus-circle"></fa>
          <span style="display:inline; ">Add new folder</span>
        </button>

        <!--<sp-widget widget="newLink"></sp-widget> -->
      </h3>

    </div>
  </div>
</div>]]></template>
    </sp_ng_template>
</record_update>
