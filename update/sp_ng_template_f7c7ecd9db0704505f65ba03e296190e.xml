        <template><![CDATA[<div class="panel panel-default" ng-init="initializeUploaderScope()">
  <div class="panel-heading">
    <h4 class="panel-title">${Add new image}</h4>
  </div>
  <div class="panel-body wrapper-xl">

    <sp-form-field form-model="data.f" field="data.f._fields.image" glide-form="getGlideForm()"></sp-form-field>

    <label class="field-label ng-binding ng-scope" for="data.imageName" title="" aria-label="" tooltip-right="true" style="" data-original-title=""><span class="field-decorations"><span class="fa fa-asterisk mandatory" ng-class="{'mandatory-filled': data.imageName.length}" title="Required" style="padding-right: .25em" aria-label="Required " aria-hidden="false"></span></span>Name</label>
    <input type="text" maxlength="40" ng-model="data.imageName" class="form-control" placeholder="Type image name"><br>

  </div>
  <div class="panel-footer">
    <button class="btn btn-danger" ng-click="close()">${Close}</button>
    <button class="btn btn-success" style="float:right" ng-click="uploadImage()">
      ${Use this image}
    </button>
  </div>
</div>

<style>
  .form-control{width:50%}
</style>]]></template>
    </sp_ng_template>
